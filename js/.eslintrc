{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "strict": 1,
        "semi": 1,
        "semi-spacing": 1,
        "no-extra-semi": 1,
        "no-var": 1,
        "no-unused-vars": 0,
        "camelcase": 1,
        "no-underscore-dangle": 1
    },
    "react": {
        "pragma": "React",
        "version": "15.0"
    }
}
